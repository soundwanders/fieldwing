		"prettier-plugin-svelte": "^2.10.1",
		"stylelint": "^15.11.0",
		"stylelint-config-recommended": "^13.0.0",
		"stylelint-config-standard": "^34.0.0",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"vite": "^5.4.0",
		"vitest": "^1.6.0"
